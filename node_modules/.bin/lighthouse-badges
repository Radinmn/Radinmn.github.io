#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/radin/Documents/GitHub/Radinmn.github.io/node_modules/.pnpm/lighthouse-badges@1.3.8/node_modules/lighthouse-badges/dist/src/node_modules:/Users/radin/Documents/GitHub/Radinmn.github.io/node_modules/.pnpm/lighthouse-badges@1.3.8/node_modules/lighthouse-badges/dist/node_modules:/Users/radin/Documents/GitHub/Radinmn.github.io/node_modules/.pnpm/lighthouse-badges@1.3.8/node_modules/lighthouse-badges/node_modules:/Users/radin/Documents/GitHub/Radinmn.github.io/node_modules/.pnpm/lighthouse-badges@1.3.8/node_modules:/Users/radin/Documents/GitHub/Radinmn.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/radin/Documents/GitHub/Radinmn.github.io/node_modules/.pnpm/lighthouse-badges@1.3.8/node_modules/lighthouse-badges/dist/src/node_modules:/Users/radin/Documents/GitHub/Radinmn.github.io/node_modules/.pnpm/lighthouse-badges@1.3.8/node_modules/lighthouse-badges/dist/node_modules:/Users/radin/Documents/GitHub/Radinmn.github.io/node_modules/.pnpm/lighthouse-badges@1.3.8/node_modules/lighthouse-badges/node_modules:/Users/radin/Documents/GitHub/Radinmn.github.io/node_modules/.pnpm/lighthouse-badges@1.3.8/node_modules:/Users/radin/Documents/GitHub/Radinmn.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lighthouse-badges/dist/src/index.js" "$@"
else
  exec node  "$basedir/../lighthouse-badges/dist/src/index.js" "$@"
fi
