#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/radin/Documents/GitHub/Radinmn.github.io/node_modules/.pnpm/astro@5.1.9_@types+node@22.10.5_jiti@2.4.2_lightningcss@1.28.2_rollup@4.31.0_terser@5.37.0_typescript@5.7.3_yaml@2.7.0/node_modules/astro/node_modules:/Users/radin/Documents/GitHub/Radinmn.github.io/node_modules/.pnpm/astro@5.1.9_@types+node@22.10.5_jiti@2.4.2_lightningcss@1.28.2_rollup@4.31.0_terser@5.37.0_typescript@5.7.3_yaml@2.7.0/node_modules:/Users/radin/Documents/GitHub/Radinmn.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/radin/Documents/GitHub/Radinmn.github.io/node_modules/.pnpm/astro@5.1.9_@types+node@22.10.5_jiti@2.4.2_lightningcss@1.28.2_rollup@4.31.0_terser@5.37.0_typescript@5.7.3_yaml@2.7.0/node_modules/astro/node_modules:/Users/radin/Documents/GitHub/Radinmn.github.io/node_modules/.pnpm/astro@5.1.9_@types+node@22.10.5_jiti@2.4.2_lightningcss@1.28.2_rollup@4.31.0_terser@5.37.0_typescript@5.7.3_yaml@2.7.0/node_modules:/Users/radin/Documents/GitHub/Radinmn.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../astro/astro.js" "$@"
else
  exec node  "$basedir/../../../astro/astro.js" "$@"
fi
